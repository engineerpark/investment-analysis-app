{
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production",
    "VERCEL_FORCE_DEPLOY": "1750519000",
    "ATOMIC_DEPLOY": "true",
    "TARGET_COMMIT": "LATEST"
  },
  "functions": {
    "pages/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Force-Deploy",
          "value": "1750519000"
        },
        {
          "key": "X-Target-Commit",
          "value": "LATEST"
        }
      ]
    }
  ]
}